{
  "dashboard": {
    "id": null,
    "title": "LLM Performance Dashboard",
    "tags": ["llm-performance", "model-call-metrics", "token-usage", "response-times", "cost-analysis"],
    "timezone": "browser",
    "refresh": "30s",
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "panels": [
      {
        "id": 1,
        "title": "LLM Performance Overview",
        "type": "stat",
        "gridPos": { "h": 8, "w": 24, "x": 0, "y": 0 },
        "targets": [
          {
            "expr": "sum(rate(llm_requests_total[5m]))",
            "legendFormat": "LLM Requests/sec",
            "refId": "A"
          },
          {
            "expr": "llm_success_rate * 100",
            "legendFormat": "LLM Success Rate %",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.95, rate(llm_response_time_seconds_bucket[5m]))",
            "legendFormat": "P95 Response Time",
            "refId": "C"
          },
          {
            "expr": "llm_performance_score * 100",
            "legendFormat": "LLM Performance Score %",
            "refId": "D"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "steps": [
                { "color": "green", "value": 0 },
                { "color": "yellow", "value": 70 },
                { "color": "red", "value": 90 }
              ]
            }
          },
          "overrides": [
            {
              "matcher": { "id": "byRegexp", "options": ".*%" },
              "properties": [
                { "id": "unit", "value": "percent" }
              ]
            },
            {
              "matcher": { "id": "byName", "options": "P95 Response Time" },
              "properties": [
                { "id": "unit", "value": "s" }
              ]
            }
          ]
        }
      },
      {
        "id": 2,
        "title": "Model Call Metrics",
        "type": "graph",
        "gridPos": { "h": 8, "w": 12, "x": 0, "y": 8 },
        "targets": [
          {
            "expr": "rate(llm_requests_total[5m])",
            "legendFormat": "Requests/sec - {{model}}",
            "refId": "A"
          },
          {
            "expr": "llm_success_rate * 100",
            "legendFormat": "Success Rate % - {{model}}",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.95, rate(llm_response_time_seconds_bucket[5m]))",
            "legendFormat": "P95 Response Time - {{model}}",
            "refId": "C"
          },
          {
            "expr": "llm_accuracy_score",
            "legendFormat": "Accuracy Score - {{model}}",
            "refId": "D"
          }
        ],
        "yAxes": [
          {
            "label": "Requests/sec / Success Rate % / Response Time / Accuracy Score",
            "min": 0
          }
        ],
        "thresholds": [
          {
            "value": 70,
            "colorMode": "warning",
            "op": "lt"
          },
          {
            "value": 50,
            "colorMode": "critical",
            "op": "lt"
          }
        ]
      },
      {
        "id": 3,
        "title": "Token Usage",
        "type": "graph",
        "gridPos": { "h": 8, "w": 12, "x": 12, "y": 8 },
        "targets": [
          {
            "expr": "rate(llm_tokens_input_total[5m])",
            "legendFormat": "Input Tokens/sec - {{model}}",
            "refId": "A"
          },
          {
            "expr": "rate(llm_tokens_output_total[5m])",
            "legendFormat": "Output Tokens/sec - {{model}}",
            "refId": "B"
          },
          {
            "expr": "rate(llm_tokens_total[5m])",
            "legendFormat": "Total Tokens/sec - {{model}}",
            "refId": "C"
          },
          {
            "expr": "rate(llm_tokens_per_second[5m])",
            "legendFormat": "Tokens/sec - {{model}}",
            "refId": "D"
          }
        ],
        "yAxes": [
          {
            "label": "Tokens/sec",
            "min": 0
          }
        ]
      },
      {
        "id": 4,
        "title": "Response Times",
        "type": "stat",
        "gridPos": { "h": 8, "w": 12, "x": 0, "y": 16 },
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(llm_response_time_seconds_bucket[5m]))",
            "legendFormat": "P50 Response Time",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, rate(llm_response_time_seconds_bucket[5m]))",
            "legendFormat": "P95 Response Time",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.99, rate(llm_response_time_seconds_bucket[5m]))",
            "legendFormat": "P99 Response Time",
            "refId": "C"
          },
          {
            "expr": "llm_average_response_time",
            "legendFormat": "Avg Response Time",
            "refId": "D"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "thresholds": {
              "steps": [
                { "color": "green", "value": 0 },
                { "color": "yellow", "value": 5 },
                { "color": "red", "value": 10 }
              ]
            }
          }
        }
      },
      {
        "id": 5,
        "title": "Cost Analysis",
        "type": "graph",
        "gridPos": { "h": 8, "w": 12, "x": 12, "y": 16 },
        "targets": [
          {
            "expr": "rate(llm_cost_total[5m])",
            "legendFormat": "Cost/sec - {{model}}",
            "refId": "A"
          },
          {
            "expr": "rate(llm_cost_per_token[5m])",
            "legendFormat": "Cost per Token - {{model}}",
            "refId": "B"
          },
          {
            "expr": "rate(llm_cost_per_request[5m])",
            "legendFormat": "Cost per Request - {{model}}",
            "refId": "C"
          },
          {
            "expr": "llm_cost_efficiency_score",
            "legendFormat": "Cost Efficiency - {{model}}",
            "refId": "D"
          }
        ],
        "yAxes": [
          {
            "label": "Cost / Efficiency Score",
            "min": 0
          }
        ],
        "thresholds": [
          {
            "value": 70,
            "colorMode": "warning",
            "op": "lt"
          },
          {
            "value": 50,
            "colorMode": "critical",
            "op": "lt"
          }
        ]
      },
      {
        "id": 6,
        "title": "Model Distribution",
        "type": "piechart",
        "gridPos": { "h": 8, "w": 12, "x": 0, "y": 24 },
        "targets": [
          {
            "expr": "rate(llm_requests_total[5m])",
            "legendFormat": "{{model}}",
            "refId": "A"
          }
        ],
        "options": {
          "pieType": "donut",
          "displayLabels": ["percent", "value"],
          "legend": {
            "displayMode": "table",
            "placement": "right"
          }
        }
      },
      {
        "id": 7,
        "title": "LLM Performance Metrics",
        "type": "graph",
        "gridPos": { "h": 8, "w": 12, "x": 12, "y": 24 },
        "targets": [
          {
            "expr": "llm_performance_score",
            "legendFormat": "Performance Score - {{performance_type}}",
            "refId": "A"
          },
          {
            "expr": "llm_efficiency_score",
            "legendFormat": "Efficiency Score - {{performance_type}}",
            "refId": "B"
          },
          {
            "expr": "llm_reliability_score",
            "legendFormat": "Reliability Score - {{performance_type}}",
            "refId": "C"
          },
          {
            "expr": "llm_scalability_score",
            "legendFormat": "Scalability Score - {{performance_type}}",
            "refId": "D"
          }
        ],
        "yAxes": [
          {
            "label": "Score",
            "min": 0
          }
        ],
        "thresholds": [
          {
            "value": 70,
            "colorMode": "warning",
            "op": "lt"
          },
          {
            "value": 50,
            "colorMode": "critical",
            "op": "lt"
          }
        ]
      },
      {
        "id": 8,
        "title": "LLM Details",
        "type": "table",
        "gridPos": { "h": 10, "w": 24, "x": 0, "y": 32 },
        "targets": [
          {
            "expr": "rate(llm_requests_total[5m])",
            "legendFormat": "{{model}}",
            "refId": "Requests",
            "format": "table"
          },
          {
            "expr": "rate(llm_tokens_total[5m])",
            "legendFormat": "{{model}}",
            "refId": "Tokens",
            "format": "table"
          },
          {
            "expr": "histogram_quantile(0.95, rate(llm_response_time_seconds_bucket[5m]))",
            "legendFormat": "{{model}}",
            "refId": "Latency",
            "format": "table"
          },
          {
            "expr": "llm_success_rate * 100",
            "legendFormat": "{{model}}",
            "refId": "Success",
            "format": "table"
          },
          {
            "expr": "rate(llm_cost_total[5m])",
            "legendFormat": "{{model}}",
            "refId": "Cost",
            "format": "table"
          },
          {
            "expr": "llm_performance_score",
            "legendFormat": "{{performance_type}}",
            "refId": "Performance",
            "format": "table"
          }
        ],
        "transformations": [
          {
            "id": "merge",
            "options": {}
          },
          {
            "id": "organize",
            "options": {
              "excludeByName": {
                "Time": true,
                "__name__": true
              },
              "indexByName": {
                "model": 0,
                "performance_type": 1,
                "Value #Requests": 2,
                "Value #Tokens": 3,
                "Value #Latency": 4,
                "Value #Success": 5,
                "Value #Cost": 6,
                "Value #Performance": 7
              },
              "renameByName": {
                "model": "Model",
                "performance_type": "Performance Type",
                "Value #Requests": "Requests/sec",
                "Value #Tokens": "Tokens/sec",
                "Value #Latency": "P95 Latency (s)",
                "Value #Success": "Success Rate %",
                "Value #Cost": "Cost/sec",
                "Value #Performance": "Performance Score"
              }
            }
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "align": "center",
              "displayMode": "color-background"
            }
          },
          "overrides": [
            {
              "matcher": { "id": "byName", "options": "P95 Latency (s)" },
              "properties": [
                {
                  "id": "thresholds",
                  "value": {
                    "steps": [
                      { "color": "green", "value": 0 },
                      { "color": "yellow", "value": 5 },
                      { "color": "red", "value": 10 }
                    ]
                  }
                }
              ]
            },
            {
              "matcher": { "id": "byName", "options": "Success Rate %" },
              "properties": [
                {
                  "id": "thresholds",
                  "value": {
                    "steps": [
                      { "color": "red", "value": 0 },
                      { "color": "yellow", "value": 70 },
                      { "color": "green", "value": 90 }
                    ]
                  }
                }
              ]
            },
            {
              "matcher": { "id": "byName", "options": "Performance Score" },
              "properties": [
                {
                  "id": "thresholds",
                  "value": {
                    "steps": [
                      { "color": "red", "value": 0 },
                      { "color": "yellow", "value": 70 },
                      { "color": "green", "value": 90 }
                    ]
                  }
                }
              ]
            }
          ]
        }
      },
      {
        "id": 9,
        "title": "LLM Performance Score",
        "type": "stat",
        "gridPos": { "h": 8, "w": 12, "x": 0, "y": 42 },
        "targets": [
          {
            "expr": "100 - (((1 - llm_success_rate) * 100 * 30) - (histogram_quantile(0.95, rate(llm_response_time_seconds_bucket[5m])) * 20) - ((1 - llm_accuracy_score) * 100 * 25) - ((1 - llm_cost_efficiency_score) * 100 * 25))",
            "legendFormat": "LLM Performance Score",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "min": 0,
            "max": 100,
            "thresholds": {
              "steps": [
                { "color": "red", "value": 0 },
                { "color": "yellow", "value": 70 },
                { "color": "green", "value": 90 }
              ]
            }
          }
        }
      },
      {
        "id": 10,
        "title": "LLM Trends",
        "type": "graph",
        "gridPos": { "h": 8, "w": 12, "x": 12, "y": 42 },
        "targets": [
          {
            "expr": "avg_over_time(sum(rate(llm_requests_total[5m]))[24h])",
            "legendFormat": "Avg Requests/sec (24h)",
            "refId": "A"
          },
          {
            "expr": "avg_over_time(llm_success_rate * 100[24h])",
            "legendFormat": "Avg Success Rate % (24h)",
            "refId": "B"
          },
          {
            "expr": "avg_over_time(histogram_quantile(0.95, rate(llm_response_time_seconds_bucket[5m]))[24h])",
            "legendFormat": "Avg P95 Latency (24h)",
            "refId": "C"
          },
          {
            "expr": "avg_over_time(llm_performance_score * 100[24h])",
            "legendFormat": "Avg Performance Score % (24h)",
            "refId": "D"
          }
        ],
        "yAxes": [
          {
            "label": "Requests/sec / Success Rate % / Latency / Performance %",
            "min": 0
          }
        ]
      },
      {
        "id": 11,
        "title": "Context Usage",
        "type": "graph",
        "gridPos": { "h": 8, "w": 12, "x": 0, "y": 50 },
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(llm_context_usage_ratio_bucket[5m])) * 100",
            "legendFormat": "P50 Context Usage % - {{model}}",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, rate(llm_context_usage_ratio_bucket[5m])) * 100",
            "legendFormat": "P95 Context Usage % - {{model}}",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.99, rate(llm_context_usage_ratio_bucket[5m])) * 100",
            "legendFormat": "P99 Context Usage % - {{model}}",
            "refId": "C"
          },
          {
            "expr": "llm_average_context_usage_ratio * 100",
            "legendFormat": "Avg Context Usage % - {{model}}",
            "refId": "D"
          }
        ],
        "yAxes": [
          {
            "label": "Context Usage %",
            "min": 0,
            "max": 100
          }
        ],
        "thresholds": [
          {
            "value": 80,
            "colorMode": "warning",
            "op": "gt"
          },
          {
            "value": 95,
            "colorMode": "critical",
            "op": "gt"
          }
        ]
      },
      {
        "id": 12,
        "title": "LLM Optimization",
        "type": "graph",
        "gridPos": { "h": 8, "w": 12, "x": 12, "y": 50 },
        "targets": [
          {
            "expr": "rate(llm_optimization_events_total[5m])",
            "legendFormat": "Optimization Events/sec - {{optimization_type}}",
            "refId": "A"
          },
          {
            "expr": "llm_optimization_success_rate * 100",
            "legendFormat": "Optimization Success Rate % - {{optimization_type}}",
            "refId": "B"
          },
          {
            "expr": "llm_optimization_improvement_score",
            "legendFormat": "Optimization Improvement - {{optimization_type}}",
            "refId": "C"
          },
          {
            "expr": "llm_optimization_efficiency_score",
            "legendFormat": "Optimization Efficiency - {{optimization_type}}",
            "refId": "D"
          }
        ],
        "yAxes": [
          {
            "label": "Events/sec / Success Rate % / Improvement Score / Efficiency Score",
            "min": 0
          }
        ],
        "thresholds": [
          {
            "value": 70,
            "colorMode": "warning",
            "op": "lt"
          },
          {
            "value": 50,
            "colorMode": "critical",
            "op": "lt"
          }
        ]
      }
    ],
    "templating": {
      "list": [
        {
          "name": "model",
          "type": "query",
          "query": "label_values(llm_requests_total, model)",
          "refresh": 1,
          "includeAll": true,
          "multi": true
        },
        {
          "name": "performance_type",
          "type": "query",
          "query": "label_values(llm_performance_score, performance_type)",
          "refresh": 1,
          "includeAll": true,
          "multi": true
        },
        {
          "name": "optimization_type",
          "type": "query",
          "query": "label_values(llm_optimization_events_total, optimization_type)",
          "refresh": 1,
          "includeAll": true,
          "multi": true
        }
      ]
    },
    "annotations": {
      "list": [
        {
          "name": "LLM Alerts",
          "datasource": "Prometheus",
          "expr": "ALERTS{alertname=~\".*LLM.*Performance.*|.*Model.*Call.*Metrics.*|.*Token.*Usage.*|.*Response.*Times.*|.*Cost.*Analysis.*\",alertstate=\"firing\"}",
          "iconColor": "red",
          "titleFormat": "LLM Alert: {{alertname}}"
        },
        {
          "name": "Low LLM Success Rate",
          "datasource": "Prometheus",
          "expr": "llm_success_rate * 100 < 70",
          "iconColor": "orange",
          "titleFormat": "Low LLM Success Rate: {{model}}"
        },
        {
          "name": "High LLM Latency",
          "datasource": "Prometheus",
          "expr": "histogram_quantile(0.95, rate(llm_response_time_seconds_bucket[5m])) > 10",
          "iconColor": "orange",
          "titleFormat": "High LLM Latency: {{model}}"
        },
        {
          "name": "High Context Usage",
          "datasource": "Prometheus",
          "expr": "llm_average_context_usage_ratio * 100 > 80",
          "iconColor": "orange",
          "titleFormat": "High Context Usage: {{model}}"
        },
        {
          "name": "Low LLM Cost Efficiency",
          "datasource": "Prometheus",
          "expr": "llm_cost_efficiency_score < 70",
          "iconColor": "orange",
          "titleFormat": "Low LLM Cost Efficiency: {{model}}"
        }
      ]
    }
  }
}