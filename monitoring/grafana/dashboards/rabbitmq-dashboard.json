{
  "dashboard": {
    "id": null,
    "title": "RabbitMQ Message Broker Monitoring",
    "tags": ["rabbitmq", "messaging", "agents", "a2a-communication"],
    "timezone": "browser",
    "refresh": "30s",
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "panels": [
      {
        "id": 1,
        "title": "RabbitMQ Node Status",
        "type": "stat",
        "gridPos": { "h": 8, "w": 8, "x": 0, "y": 0 },
        "targets": [
          {
            "expr": "rabbitmq_up",
            "legendFormat": "Node Status",
            "refId": "A"
          },
          {
            "expr": "rabbitmq_node_uptime_seconds",
            "legendFormat": "Uptime (s)",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "steps": [
                { "color": "red", "value": 0 },
                { "color": "green", "value": 1 }
              ]
            }
          },
          "overrides": [
            {
              "matcher": { "id": "byName", "options": "Node Status" },
              "properties": [
                { "id": "mappings", "value": [
                  { "options": { "0": { "text": "DOWN", "color": "red" } } },
                  { "options": { "1": { "text": "UP", "color": "green" } } }
                ]}
              ]
            },
            {
              "matcher": { "id": "byName", "options": "Uptime (s)" },
              "properties": [
                { "id": "unit", "value": "s" }
              ]
            }
          ]
        }
      },
      {
        "id": 2,
        "title": "Queue Depth by Queue",
        "type": "graph",
        "gridPos": { "h": 8, "w": 16, "x": 8, "y": 0 },
        "targets": [
          {
            "expr": "rabbitmq_queue_messages",
            "legendFormat": "{{queue}} - Messages",
            "refId": "A"
          },
          {
            "expr": "rabbitmq_queue_messages_ready",
            "legendFormat": "{{queue}} - Ready",
            "refId": "B"
          },
          {
            "expr": "rabbitmq_queue_messages_unacknowledged",
            "legendFormat": "{{queue}} - Unacked",
            "refId": "C"
          }
        ],
        "yAxes": [
          {
            "label": "Message Count",
            "min": 0
          }
        ],
        "thresholds": [
          {
            "value": 1000,
            "colorMode": "critical",
            "op": "gt"
          },
          {
            "value": 5000,
            "colorMode": "critical",
            "op": "gt"
          }
        ]
      },
      {
        "id": 3,
        "title": "Message Rates",
        "type": "graph",
        "gridPos": { "h": 8, "w": 12, "x": 0, "y": 8 },
        "targets": [
          {
            "expr": "rate(rabbitmq_queue_messages_published_total[5m])",
            "legendFormat": "{{queue}} - Published/s",
            "refId": "A"
          },
          {
            "expr": "rate(rabbitmq_queue_messages_delivered_total[5m])",
            "legendFormat": "{{queue}} - Delivered/s",
            "refId": "B"
          },
          {
            "expr": "rate(rabbitmq_queue_messages_acknowledged_total[5m])",
            "legendFormat": "{{queue}} - Acked/s",
            "refId": "C"
          }
        ],
        "yAxes": [
          {
            "label": "Messages/sec",
            "min": 0
          }
        ]
      },
      {
        "id": 4,
        "title": "Consumer Activity",
        "type": "graph",
        "gridPos": { "h": 8, "w": 12, "x": 12, "y": 8 },
        "targets": [
          {
            "expr": "rabbitmq_queue_consumers",
            "legendFormat": "{{queue}} - Consumers",
            "refId": "A"
          },
          {
            "expr": "rate(rabbitmq_queue_consumer_utilisation[5m]) * 100",
            "legendFormat": "{{queue}} - Utilization %",
            "refId": "B"
          }
        ],
        "yAxes": [
          {
            "label": "Count / Percentage",
            "min": 0
          }
        ]
      },
      {
        "id": 5,
        "title": "Memory & Disk Usage",
        "type": "graph",
        "gridPos": { "h": 8, "w": 12, "x": 0, "y": 16 },
        "targets": [
          {
            "expr": "(rabbitmq_node_mem_used / rabbitmq_node_mem_limit) * 100",
            "legendFormat": "Memory Usage %",
            "refId": "A"
          },
          {
            "expr": "(rabbitmq_node_disk_free / rabbitmq_node_disk_free_limit) * 100",
            "legendFormat": "Disk Free %",
            "refId": "B"
          }
        ],
        "yAxes": [
          {
            "label": "Percentage",
            "min": 0,
            "max": 100
          }
        ],
        "thresholds": [
          {
            "value": 80,
            "colorMode": "critical",
            "op": "gt"
          }
        ]
      },
      {
        "id": 6,
        "title": "Connection Statistics",
        "type": "stat",
        "gridPos": { "h": 8, "w": 12, "x": 12, "y": 16 },
        "targets": [
          {
            "expr": "rabbitmq_connections",
            "legendFormat": "Active Connections",
            "refId": "A"
          },
          {
            "expr": "rabbitmq_channels",
            "legendFormat": "Active Channels",
            "refId": "B"
          },
          {
            "expr": "rate(rabbitmq_connections_opened_total[5m])",
            "legendFormat": "Connections Opened/s",
            "refId": "C"
          },
          {
            "expr": "rate(rabbitmq_connections_closed_total[5m])",
            "legendFormat": "Connections Closed/s",
            "refId": "D"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "steps": [
                { "color": "green", "value": 0 },
                { "color": "yellow", "value": 100 },
                { "color": "red", "value": 500 }
              ]
            }
          }
        }
      },
      {
        "id": 7,
        "title": "Agent Communication Patterns",
        "type": "graph",
        "gridPos": { "h": 8, "w": 24, "x": 0, "y": 24 },
        "targets": [
          {
            "expr": "rate(rabbitmq_queue_messages_published_total{queue=~\".*agent.*\"}[5m])",
            "legendFormat": "Agent {{queue}} - Published/s",
            "refId": "A"
          },
          {
            "expr": "rate(rabbitmq_queue_messages_delivered_total{queue=~\".*agent.*\"}[5m])",
            "legendFormat": "Agent {{queue}} - Delivered/s",
            "refId": "B"
          }
        ],
        "yAxes": [
          {
            "label": "Messages/sec",
            "min": 0
          }
        ]
      },
      {
        "id": 8,
        "title": "Queue Details Table",
        "type": "table",
        "gridPos": { "h": 10, "w": 24, "x": 0, "y": 32 },
        "targets": [
          {
            "expr": "rabbitmq_queue_messages",
            "legendFormat": "{{queue}}",
            "refId": "Messages",
            "format": "table"
          },
          {
            "expr": "rabbitmq_queue_consumers",
            "legendFormat": "{{queue}}",
            "refId": "Consumers",
            "format": "table"
          },
          {
            "expr": "rate(rabbitmq_queue_messages_published_total[5m])",
            "legendFormat": "{{queue}}",
            "refId": "PublishRate",
            "format": "table"
          },
          {
            "expr": "rate(rabbitmq_queue_messages_delivered_total[5m])",
            "legendFormat": "{{queue}}",
            "refId": "DeliveryRate",
            "format": "table"
          }
        ],
        "transformations": [
          {
            "id": "merge",
            "options": {}
          },
          {
            "id": "organize",
            "options": {
              "excludeByName": {
                "Time": true,
                "__name__": true,
                "instance": true,
                "job": true
              },
              "indexByName": {
                "queue": 0,
                "Value #Messages": 1,
                "Value #Consumers": 2,
                "Value #PublishRate": 3,
                "Value #DeliveryRate": 4
              },
              "renameByName": {
                "queue": "Queue Name",
                "Value #Messages": "Messages",
                "Value #Consumers": "Consumers",
                "Value #PublishRate": "Publish Rate/s",
                "Value #DeliveryRate": "Delivery Rate/s"
              }
            }
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "align": "center",
              "displayMode": "color-background"
            }
          },
          "overrides": [
            {
              "matcher": { "id": "byName", "options": "Messages" },
              "properties": [
                {
                  "id": "thresholds",
                  "value": {
                    "steps": [
                      { "color": "green", "value": 0 },
                      { "color": "yellow", "value": 100 },
                      { "color": "orange", "value": 1000 },
                      { "color": "red", "value": 5000 }
                    ]
                  }
                }
              ]
            },
            {
              "matcher": { "id": "byName", "options": "Consumers" },
              "properties": [
                {
                  "id": "thresholds",
                  "value": {
                    "steps": [
                      { "color": "red", "value": 0 },
                      { "color": "green", "value": 1 }
                    ]
                  }
                }
              ]
            }
          ]
        }
      },
      {
        "id": 9,
        "title": "Message Latency (if available)",
        "type": "graph",
        "gridPos": { "h": 8, "w": 12, "x": 0, "y": 42 },
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(rabbitmq_queue_message_latency_seconds_bucket[5m]))",
            "legendFormat": "{{queue}} - P50 Latency",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, rate(rabbitmq_queue_message_latency_seconds_bucket[5m]))",
            "legendFormat": "{{queue}} - P95 Latency",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.99, rate(rabbitmq_queue_message_latency_seconds_bucket[5m]))",
            "legendFormat": "{{queue}} - P99 Latency",
            "refId": "C"
          }
        ],
        "yAxes": [
          {
            "label": "Seconds",
            "min": 0
          }
        ],
        "thresholds": [
          {
            "value": 0.1,
            "colorMode": "critical",
            "op": "gt"
          }
        ]
      },
      {
        "id": 10,
        "title": "A2A Communication Health",
        "type": "stat",
        "gridPos": { "h": 8, "w": 12, "x": 12, "y": 42 },
        "targets": [
          {
            "expr": "sum(rabbitmq_queue_messages{queue=~\".*a2a.*|.*agent.*|.*collaboration.*\"})",
            "legendFormat": "Total A2A Messages",
            "refId": "A"
          },
          {
            "expr": "sum(rate(rabbitmq_queue_messages_published_total{queue=~\".*a2a.*|.*agent.*|.*collaboration.*\"}[5m]))",
            "legendFormat": "A2A Publish Rate/s",
            "refId": "B"
          },
          {
            "expr": "sum(rate(rabbitmq_queue_messages_delivered_total{queue=~\".*a2a.*|.*agent.*|.*collaboration.*\"}[5m]))",
            "legendFormat": "A2A Delivery Rate/s",
            "refId": "C"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "steps": [
                { "color": "green", "value": 0 },
                { "color": "yellow", "value": 1000 },
                { "color": "red", "value": 10000 }
              ]
            }
          }
        }
      }
    ],
    "templating": {
      "list": [
        {
          "name": "instance",
          "type": "query",
          "query": "label_values(rabbitmq_up, instance)",
          "refresh": 1,
          "includeAll": false,
          "multi": false
        },
        {
          "name": "queue",
          "type": "query",
          "query": "label_values(rabbitmq_queue_messages, queue)",
          "refresh": 1,
          "includeAll": true,
          "multi": true
        }
      ]
    },
    "annotations": {
      "list": [
        {
          "name": "RabbitMQ Alerts",
          "datasource": "Prometheus",
          "expr": "ALERTS{alertname=~\".*RabbitMQ.*\",alertstate=\"firing\"}",
          "iconColor": "red",
          "titleFormat": "Alert: {{alertname}}"
        },
        {
          "name": "Queue Depth Alerts",
          "datasource": "Prometheus",
          "expr": "rabbitmq_queue_messages > 1000",
          "iconColor": "orange",
          "titleFormat": "High Queue Depth: {{queue}}"
        }
      ]
    }
  }
}