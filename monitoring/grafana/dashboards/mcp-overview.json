    "panels": [
      {
        "id": 1,
        "title": "System Health Status",
        "type": "stat",
        "gridPos": { "h": 8, "w": 24, "x": 0, "y": 0 },
        "targets": [
          {
            "expr": "up{job=~\"mcp-services|frontend|llm-.*|rabbitmq|postgres|redis\"}",
            "legendFormat": "{{job}} - {{instance}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "steps": [
                { "color": "red", "value": 0 },
                { "color": "green", "value": 1 }
              ]
            }
          },
          "overrides": [
            {
              "matcher": { "id": "byName", "options": ".*" },
              "properties": [
                { "id": "mappings", "value": [
                  { "options": { "0": { "text": "DOWN", "color": "red" } } },
                  { "options": { "1": { "text": "UP", "color": "green" } } }
