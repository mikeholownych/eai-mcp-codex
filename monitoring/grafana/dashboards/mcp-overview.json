          {
            "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket{job=~\"mcp-services|frontend\"}[5m]))",
            "legendFormat": "{{job}} - P95 Response Time",
            "refId": "A"
          }
        ],
        "yAxes": [
          {
            "label": "Seconds",
            "min": 0
          }
        ],
        "thresholds": [
          {
            "value": 2,
