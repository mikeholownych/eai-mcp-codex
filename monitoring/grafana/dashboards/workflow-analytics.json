  "dashboard": {
    "id": null,
    "title": "MCP Workflow Analytics",
    "tags": ["mcp", "workflow", "analytics", "orchestration"],
    "timezone": "browser",
    "refresh": "30s",
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "panels": [
      {
        "id": 1,
        "title": "Workflow Overview",
        "type": "stat",
        "gridPos": { "h": 8, "w": 24, "x": 0, "y": 0 },
        "targets": [
          {
            "expr": "mcp_workflows_active_total",
            "legendFormat": "Active Workflows",
            "refId": "A"
          },
          {
            "expr": "mcp_workflows_completed_total",
            "legendFormat": "Completed Workflows",
            "refId": "B"
          },
          {
            "expr": "mcp_workflows_failed_total",
            "legendFormat": "Failed Workflows",
            "refId": "C"
          },
          {
            "expr": "mcp_workflows_pending_total",
            "legendFormat": "Pending Workflows",
            "refId": "D"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "steps": [
                { "color": "green", "value": 0 },
                { "color": "yellow", "value": 10 },
                { "color": "red", "value": 50 }
              ]
            }
          },
          "overrides": [
            {
              "matcher": { "id": "byName", "options": "Failed Workflows" },
              "properties": [
                {
                  "id": "thresholds",
                  "value": {
                    "steps": [
                      { "color": "green", "value": 0 },
                      { "color": "yellow", "value": 1 },
                      { "color": "red", "value": 5 }
                    ]
                  }
                }
              ]
            }
          ]
        }
      },
      {
        "id": 2,
        "title": "Workflow Execution Rate",
        "type": "graph",
        "gridPos": { "h": 8, "w": 12, "x": 0, "y": 8 },
        "targets": [
          {
            "expr": "rate(mcp_workflows_started_total[5m])",
            "legendFormat": "Workflows Started/sec",
            "refId": "A"
          },
          {
            "expr": "rate(mcp_workflows_completed_total[5m])",
            "legendFormat": "Workflows Completed/sec",
            "refId": "B"
          },
          {
            "expr": "rate(mcp_workflows_failed_total[5m])",
            "legendFormat": "Workflows Failed/sec",
            "refId": "C"
          }
        ],
        "yAxes": [
          {
            "label": "Workflows/sec",
            "min": 0
          }
        ]
      },
      {
        "id": 3,
        "title": "Workflow Duration",
        "type": "graph",
        "gridPos": { "h": 8, "w": 12, "x": 12, "y": 8 },
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(mcp_workflow_duration_seconds_bucket[5m]))",
            "legendFormat": "P50 Duration",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, rate(mcp_workflow_duration_seconds_bucket[5m]))",
            "legendFormat": "P95 Duration",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.99, rate(mcp_workflow_duration_seconds_bucket[5m]))",
            "legendFormat": "P99 Duration",
            "refId": "C"
          }
        ],
        "yAxes": [
          {
            "label": "Seconds",
            "min": 0
          }
        ],
        "thresholds": [
          {
            "value": 60,
            "colorMode": "warning",
            "op": "gt"
          },
          {
            "value": 300,
            "colorMode": "critical",
            "op": "gt"
          }
        ]
      },
      {
        "id": 4,
        "title": "Workflow Steps Analysis",
        "type": "graph",
        "gridPos": { "h": 8, "w": 12, "x": 0, "y": 16 },
        "targets": [
          {
            "expr": "mcp_workflow_steps_active_total",
            "legendFormat": "Active Steps",
            "refId": "A"
          },
          {
            "expr": "rate(mcp_workflow_steps_completed_total[5m])",
            "legendFormat": "Steps Completed/sec",
            "refId": "B"
          },
          {
            "expr": "rate(mcp_workflow_steps_failed_total[5m])",
            "legendFormat": "Steps Failed/sec",
            "refId": "C"
          }
        ],
        "yAxes": [
          {
            "label": "Steps / Rate",
            "min": 0
          }
        ]
      },
      {
        "id": 5,
        "title": "Agent Handoff Metrics",
        "type": "graph",
